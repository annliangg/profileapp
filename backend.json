{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": {
      "type": "object",
      "properties": {
        "age": {
          "type": "string",
          "pattern": "^[0-9]+$"
        },
        "birthday": {
          "type": "string",
          "pattern": "^\\d{2}/\\d{2}/\\d{4}$"
        },
        "image": {
          "type": "string",
          "format": "uri"
        },
        "name": {
          "type": "string"
        },
        "state": {
          "type": "string",
          "pattern": "^[A-Z]{2}$"
        }
      },
      "required": ["age", "birthday", "image", "name", "state"]
    }
  }
  